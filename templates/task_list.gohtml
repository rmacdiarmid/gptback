{{define "task_list.gohtml"}}
	<h1>Task List</h1>
	<table>
		<thead>
			<tr>
				<th>ID</th>
				<th>Title</th>
				<th>Description</th>
			</tr>
		</thead>
		<tbody>
			{{range .}}
			<tr>
				<td>{{.ID}}</td>
				<td>{{.Title}}</td>
				<td>{{.Description}}</td>
			</tr>
			{{end}}
		</tbody>
	</table>
{{end}}